{
  "name": "youtube",
  "version": "1.0",
  "description": "Build an Extension!",
  "manifest_version": 3,
  "permissions": [
    "webRequest",
    "bookmarks",
    "activeTab",
    "scripting",
    "nativeMessaging",
    "declarativeNetRequest",
    "declarativeNetRequestWithHostAccess"
  ],
  "content_scripts": [
    {
      "js": [
        "index.js"
      ],
   "matches": ["http://*/*", "https://*/*"]
    }
  ],
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ],
"action": {}
}
